{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "https://blog.driggl.now.sh/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://driggl.com"
      }
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "FB_APP_ID": "@fb-app-id",
    "GA_UA": "@ga-ua"
  },
  "build": {
    "env": {
      "FB_APP_ID": "@fb-app-id",
      "GA_UA": "@ga-ua"
    }
  }
}